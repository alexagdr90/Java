public class UsoArrays2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String [] paises= new String[8];
		
		paises[0]="España";
		paises[1]="Italia";
		paises[2]="Portugal";
		paises[3]="Islandia";
		paises[4]="Grecia";
		paises[5]="Mexico";
		paises[6]="Argentina";
		paises[7]="Irlandia";

		/*for(int i=0;i<paises.length;i++) {
			System.out.println("Pais "+(i+1)+" "+paises[i]);
		}*/
		
		for(String elementos:paises) {
			int i=0;
			System.out.println("Pais "+i+": "+elementos);
			i++;
		}
		
	}

}
